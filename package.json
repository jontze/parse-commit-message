{
  "name": "parse-commit-message",
  "version": "5.0.0",
  "licenseStart": 2018,
  "license": "MPL-2.0",
  "description": "Extensible parser for git commit messages following Conventional Commits Specification",
  "author": "Charlike Mike Reagent <opensource@tunnckocore.com> (https://tunnckocore.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/jontze/parse-commit-message.git"
  },
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "ci": "npm run format:check && npm run build && npm run test",
    "build": "tsc",
    "format": "prettier -c .prettierrc.json --write ./src",
    "format:check": "prettier -c .prettierrc.json --check ./src",
    "test": "jest --passWithNoTests --coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "commit message parser",
    "commit messages",
    "commit-message",
    "commit-parser",
    "commits",
    "commits-parser",
    "conventional",
    "conventional-commits",
    "conventionalcommits",
    "parse commit message",
    "parse-commit",
    "parse-commit-message",
    "parse-commits",
    "parser"
  ],
  "dependencies": {
    "immer": "^10.0.4"
  }
}
